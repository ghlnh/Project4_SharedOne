<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
 <mapper namespace="com.sharedOne.mapper.master.ProductMapper">
 
 	<select id="selectProductList" resultType="com.sharedOne.domain.master.ProductDto">
 	<!-- productCode, productName, productType, weight, size, price, unit, content, inserted -->
		SELECT 
			productCode,
			productName,
			productType,
			weight,
			size,
			price,
			unit,
			content,
			inserted
		FROM 
			Product
			
 	</select>
 	
 
 </mapper>